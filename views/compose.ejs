<%- include("partials/header")-%>
<h1>Compose</h1>

<form class="composePost" action="/" method="post">
<div class="mb-3">
  <label for="postTitle" class="form-label">Title</label>
  <input type="text" class="form-control" name="postTitle">
</div>
<div class="mb-3">
  <label for="postBody" class="form-label">Post</label>
  <textarea class="form-control" name="postBody" rows="3"></textarea>
</div>
<button type="submit" class="btn btn-primary btn-publish" name="publish">Publish</button>
</form>

<%- include("partials/footer")-%>
